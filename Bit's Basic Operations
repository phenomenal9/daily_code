class Solution {
  
    public int XOR(int n, int m)
    { 
        // Code here
        return n ^ m;
    }
    public int check(int a, int b) {
        // Code here
        while(a > 1) {
            b = b >> 1;
            a--;
        }
        return b & 1;
    }
    public int setBit(int c, int d) {
        // Code here
        int n = 1;
        while(c > 0) {
            n = n << 1;
            c--;
        }
        
        return n | d; 
    }
}
