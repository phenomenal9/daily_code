public static Node reverseDLL(Node  head)
{
    //Your code here
    if(head == null || head.next == null) return head;
    Node initial = head;
    Node prevNode = null;
    Node nextNode = head;
    Node curr = head;
    while(curr != null)
    {
        nextNode = curr.next;
        curr.next = prevNode;
        curr.prev = nextNode;
        prevNode = curr;
        curr = nextNode;
    }
    
    return prevNode;
}
